#include <stdio.h>
#include <string.h>

int main() {
    char str[] = "MY NAME MY NAME";
    char str1[50][50];
    int n = 0;
    int i, j, count = 0;
    int len = strlen(str);

    // Split the string into words
    char *ptr = strtok(str, " ");
    while (ptr != NULL && n < len) {
        strcpy(str1[n], ptr);
        n++;
        ptr = strtok(NULL, " ");
    }

    // Check for duplicate words
    for (i = 0; i < n; i++) {
        for (j = i + 1; j < n; j++) {
            if (strcmp(str1[i], str1[j]) == 0) {
                printf("Duplicate word: %s\n", str1[i]);
            }
        }
    }

    return 0;
}

.....................

#include <stdio.h>
#include <string.h>

int main() {
    char str[] = "MY NAME MY NAME";
    char str1[50][50];
    int n = 0;
    int i, j, count = 0;
    int len = strlen(str);
    char word[50];

    // Split the string into words
    for (i = 0; i < len; i++) {
        if (str[i] != ' ') {
            word[count] = str[i];
            count++;
        } else {
            word[count] = '\0';
            strcpy(str1[n], word);
            n++;
            count = 0;
        }
    }
    word[count] = '\0';
    strcpy(str1[n], word);
    n++;

    // Check for duplicate words
    for (i = 0; i < n; i++) {
        for (j = i + 1; j < n; j++) {
            if (strcmp(str1[i], str1[j]) == 0)
                printf("Duplicate word: %s\n", str1[i]);
        }
    }

    return 0;
}
